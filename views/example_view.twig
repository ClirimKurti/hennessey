<!DOCTYPE html>
<html>
<head>
    <title>{{ title }}</title>
     {{ wp_head|raw }}   
</head>
<body>
   <h1 class="bg-pink-100 text-lg">{{ title }}</h1>
     {{ wp_footer|raw }}  
    

<div
    x-data="{
        search: '',
 
        items: ['foo', 'bar', 'baz'],
 
        get filteredItems() {
            return this.items.filter(
                i => i.startsWith(this.search)
            )
        }
    }"
>
    <input x-model="search" placeholder="Search...">
 
    <ul>
        <template x-for="item in filteredItems" :key="item">
            <li x-text="item"></li>
        </template>
    </ul>
</div>
</body>
</html>
    